    }

    #time_seeking p {
        color: black;
        font-family: Arial, Helvetica, sans-serif;
        font-size: 18px;
        font-weight: bold;
        margin: 0;
    }

    #time_seeking::before {
        content: "";
        position: absolute;
        width: 12px;
        height: 12px;
        background-color: white;
        bottom: -6px;
        left: 19px;
        border-top-left-radius: 50%;
        transform: rotate(45deg);
