        top: -8px;
        width: 20px;
        height: 20px;
        border-radius: 50%;
        background-color: red;
        display: none;
    }

    #time_seeking {
        position: absolute;
        height: 25px;
        top: -40px;
        right: -25px;
        border-radius: 5px;
        background-color: white;
        display: none;
    }

    #time_seeking>div {
        display: flex;
