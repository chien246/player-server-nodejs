    .controls {
        position: absolute;
        bottom: 5px;
        margin: 0;
        height: 50px;
        width: 100%;
        transition: 0.5s ease-in;
    }

    .progress_bar {
        position: relative;
        width: 99%;
        height: 4px;
        background-color: rgba(255, 255, 255, 0.6);
        margin: auto;
        cursor: pointer;
    }

    #progress_bar_seeking {
        position: absolute;
        top: -8px;
